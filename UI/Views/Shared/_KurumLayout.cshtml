<!DOCTYPE html>
<html lang="tr">

<head>
    <meta charset="utf-8">
    <title>SchoolNET - Web Tabanlı Okul Yönetim Sistemi</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Web Tabanlı Okul Yönetim Sistemi" name="description">
    <meta content="Coderthemes" name="author">

    <!-- third party css -->
    <link href="~/assets/css/vendor/jquery-jvectormap-1.2.2.css" rel="stylesheet" type="text/css">
    <!-- third party css end -->
    <!-- App css -->
    <link href="~/assets/css/icons.min.css" rel="stylesheet" type="text/css">
    <link href="~/assets/css/app.min.css" rel="stylesheet" type="text/css" id="light-style">
    <link href="~/assets/css/app-dark.min.css" rel="stylesheet" type="text/css" id="dark-style">
    <!-- third party css -->

    <link href="~/assets/css/vendor/dataTables.bootstrap5.css" rel="stylesheet" type="text/css">
    <link href="~/assets/css/vendor/responsive.bootstrap5.css" rel="stylesheet" type="text/css">
    <link href="~/assets/css/vendor/buttons.bootstrap5.css" rel="stylesheet" type="text/css">
    <link href="~/assets/css/vendor/select.bootstrap5.css" rel="stylesheet" type="text/css">

    <!-- third party css end -->
    <link href="~/toastr.js/toastr.css" rel="stylesheet" />
    <link href="~/bootstrap-sweetalert/sweetalert.css" rel="stylesheet" />
    <link href="~/css/loader.css" rel="stylesheet" />
    @*<link href="~/assets/css/vendor/simplemde.min.css" rel="stylesheet" />*@

</head>

<body class="loading" data-layout-config='{"leftSideBarTheme":"dark","layoutBoxed":false, "leftSidebarCondensed":false, "leftSidebarScrollable":false,"darkMode":false, "showRightSidebarOnStart": true}'>
    <!-- Begin page -->
    <div class="wrapper">
        <!-- ========== Left Sidebar Start ========== -->
        <div class="leftside-menu">

            <!-- LOGO -->
            <a href="@Url.Action("Index","Okul")" class="logo text-center logo-light">
                Okullar
            </a>

            <!-- LOGO -->
            <a href="index.html" class="logo text-center logo-dark">
                <span class="logo-lg">
                    <img src="~/assets/images/logo-dark.png" alt="" height="16">
                </span>
                <span class="logo-sm">
                    <img src="~/assets/images/logo_sm_dark.png" alt="" height="16">
                </span>
            </a>

            <div class="h-100" id="leftside-menu-container" data-simplebar="">

                <!--- Sidemenu -->
                <ul class="side-nav">

                    <li class="side-nav-title side-nav-item">Menü</li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("Index","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Anasayfa </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("KurumDetay","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Kurum Detay </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("KullaniciListesi","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Kullanıcı Listesi </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("OgretmenListesi","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Öğretmen Listesi </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("EgitimModeli","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Eğitim Modeli </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("Basarilar","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Başarılar </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("Kataloglar","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Kataloglar </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("IcGorseller","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> İç Görseller </span>
                        </a>
                    </li>
                     <li class="side-nav-item">
                       <a href="@Url.Action("DisGorseller","Kurum")" class="side-nav-link">
                           <i class="uil-home-alt"></i>
                            <span> Dış Görseller </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("Etkinlikler","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Etkinlikler </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("Kulupler","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Kulüpler </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("Yorumlar","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> Yorumlar </span>
                        </a>
                    </li>
                    <li class="side-nav-item">
                        <a href="@Url.Action("indirimler","Kurum")" class="side-nav-link">
                            <i class="uil-home-alt"></i>
                            <span> İndirimler </span>
                        </a>
                    </li>
                     <li class="side-nav-item">
                       <a href="@Url.Action("BurslulukSinavlari","Kurum")" class="side-nav-link">
                           <i class="uil-home-alt"></i>
                            <span> Bursluluk Sınavları </span>
                        </a>
                    </li>
                </ul>

                <div class="clearfix"></div>

            </div>
            <!-- Sidebar -left -->

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <div class="content">
                <!-- Topbar Start -->
                <div class="navbar-custom">
                    <ul class="list-unstyled topbar-menu float-end mb-0">

                        <li class="dropdown notification-list">
                            <a class="nav-link dropdown-toggle nav-user arrow-none me-0" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                                <span class="account-user-avatar">
                                    <img src="~/assets/images/users/avatar-1.jpg" alt="user-image" class="rounded-circle">
                                </span>
                                <span>
                                    @*<span class="account-user-name">@Model.Ad @Model.Soyad</span>*@
                                    <span class="account-position">Founder</span>
                                </span>
                            </a>
                            <div class="dropdown-menu dropdown-menu-end dropdown-menu-animated topbar-dropdown-menu profile-dropdown">
                                <!-- item-->
                                <div class=" dropdown-header noti-title">
                                    <h6 class="text-overflow m-0">Welcome !</h6>
                                </div>

                                <!-- item-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item">
                                    <i class="mdi mdi-account-circle me-1"></i>
                                    <span>My Account</span>
                                </a>

                                <!-- item-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item">
                                    <i class="mdi mdi-account-edit me-1"></i>
                                    <span>Settings</span>
                                </a>

                                <!-- item-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item">
                                    <i class="mdi mdi-lifebuoy me-1"></i>
                                    <span>Support</span>
                                </a>

                                <!-- item-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item">
                                    <i class="mdi mdi-lock-outline me-1"></i>
                                    <span>Lock Screen</span>
                                </a>

                                <!-- item-->
                                <a href="@Url.Action("CikisYap","Kurum")" class="dropdown-item notify-item">
                                    <i class="mdi mdi-logout me-1"></i>
                                    <span>Çıkış Yap</span>
                                </a>
                            </div>
                        </li>

                    </ul>
                    <button class="button-menu-mobile open-left">
                        <i class="mdi mdi-menu"></i>
                    </button>
                </div>

                <!-- end Topbar -->
                <!-- Start Content-->
                <div class="container-fluid">


                    <!-- end page title -->
                    @RenderBody()



                </div>
                <!-- container -->

            </div>
            <!-- content -->
            <!-- Footer Start -->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6">
                            @DateTime.Now.Year © Web Tabanlı Staj Takip Uygulaması
                        </div>
                    </div>
                </div>
            </footer>
            <!-- end Footer -->

        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->


    </div>
    <!-- END wrapper -->
    <!-- Right Sidebar -->

    <div id="loading">
        <img src="~/1496.gif" />
    </div>
    <!-- /End-bar -->
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.js"></script>
    <script src="~/toastr.js/toastr.min.js"></script>
    <script src="~/bootstrap-sweetalert/sweetalert.js"></script>
    <!-- bundle -->
    <script src="~/assets/js/vendor.min.js"></script>
    <script src="~/assets/js/app.min.js"></script>

    <!-- third party js -->
    <script src="~/assets/js/vendor/apexcharts.min.js"></script>
    <script src="~/assets/js/vendor/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/assets/js/vendor/jquery-jvectormap-world-mill-en.js"></script>
    <!-- third party js ends -->
    <!-- demo app -->
    <script src="~/assets/js/pages/demo.dashboard.js"></script>
    <script src="~/assets/js/pages/demo.toastr.js"></script>
    <!-- end demo js-->
    <!-- third party js -->
    <script src="~/assets/js/vendor/jquery.dataTables.min.js"></script>
    <script src="~/assets/js/vendor/dataTables.bootstrap5.js"></script>
    <script src="~/assets/js/vendor/dataTables.responsive.min.js"></script>
    <script src="~/assets/js/vendor/responsive.bootstrap5.min.js"></script>
    <script src="~/assets/js/vendor/dataTables.buttons.min.js"></script>
    <script src="~/assets/js/vendor/buttons.bootstrap5.min.js"></script>
    <script src="~/assets/js/vendor/buttons.html5.min.js"></script>
    <script src="~/assets/js/vendor/buttons.flash.min.js"></script>
    <script src="~/assets/js/vendor/buttons.print.min.js"></script>
    <script src="~/assets/js/vendor/dataTables.keyTable.min.js"></script>
    <script src="~/assets/js/vendor/dataTables.select.min.js"></script>
    <!-- third party js ends -->
    <!-- demo app -->
    <script src="~/assets/js/pages/demo.datatable-init.js"></script>
    <!-- end demo js-->
    <!-- plugin js -->
    <script src="~/assets/js/vendor/dropzone.min.js"></script>
    <!-- init js -->
    <script src="~/assets/js/ui/component.fileupload.js"></script>
    @*<script src="~/assets/js/vendor/simplemde.min.js"></script>
    <script src="~/assets/js/pages/demo.simplemde.js"></script>*@


    @await RenderSectionAsync("Scripts", required: false)
    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });

        $(window).on('load',
            function () {
                $('#loading').hide();
            });
    </script>
</body>
</html>